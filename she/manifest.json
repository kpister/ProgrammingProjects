{
    "manifest_version": 2,
    "name": "she",
    "version": "0.1",
    "permissions": [
        "http://*/*", 
        "https://*/*"
    ],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
